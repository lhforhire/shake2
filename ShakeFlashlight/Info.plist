<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleDevelopmentRegion</key>
    <string>en</string>
    <key>CFBundleExecutable</key>
    <string>$(EXECUTABLE_NAME)</string>
    <key>CFBundleIdentifier</key>
    <string>com.example.shakeflashlight</string> <!-- Update this to your bundle identifier -->
    <key>CFBundleInfoDictionaryVersion</key>
    <string>6.0</string>
    <key>CFBundleName</key>
    <string>ShakeFlashlight</string>
    <key>CFBundlePackageType</key>
    <string>APPL</string>
    <key>CFBundleShortVersionString</key>
    <string>1.0</string>
    <key>CFBundleVersion</key>
    <string>1</string>
    <key>LSRequiresIPhoneOS</key>
    <true/>
    <key>UILaunchStoryboardName</key>
    <string>Main</string> <!-- Ensure "Main.storyboard" is included in the project -->
    <key>UIRequiredDeviceCapabilities</key>
    <array>
        <string>arm64</string> <!-- Ensure the app supports only modern devices -->
    </array>
    <key>UISupportedInterfaceOrientations</key>
    <array>
        <string>UIInterfaceOrientationPortrait</string> <!-- Supports portrait mode -->
    </array>
    <key>NSCameraUsageDescription</key>
    <string>This app needs access to the camera to control the flashlight.</string> <!-- Ensure this is accurate -->
</dict>
</plist>
